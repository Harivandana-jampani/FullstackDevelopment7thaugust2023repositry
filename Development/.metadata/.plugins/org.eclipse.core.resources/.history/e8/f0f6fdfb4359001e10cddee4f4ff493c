package com.gentech.Voidmethod;
//odd numbers from 20 to 40
class Oddnum
{
	static int num=20;
	static void shownumbers()
	{
		if(num<=40)
		{
			if(num%2==1)
			{
				System.out.println(num);

			}
			num++;
			shownumbers();
		}
	}
	//even numbers from 20 to 1

	static int num1=20;
	static void shownumber()
	{
		if(num1>=1)
		{
			if(num1%2==0)
			{
				System.out.println(num1);
			}
			num1--;
			shownumber();
		}
	}
	//divisible by 8
	static int num2=20;
	static void shownumber1()
	{
		if(num2<=100)
		{
			if(num2%8==0)
			{
				System.out.println(num2);
			}
			num2++;
			shownumber1();
		}
	}
	//display 10th table
	static int num3=1;
	static void shownumber2()
	{
		if(num3<=10)
		{
			System.out.println("10*"+num3+"="+num3*10);

			num3++;
			shownumber2();
		}
	}
	//display 5th table
	static int num4=10;
	static void shownumber3()
	{
		if(num4>=1)
		{
			System.out.println("5*"+num4+"="+num4*5);

			num4--;
			shownumber3();
		}
	}
	//count of prime numbers divisible by 10

	static int n=1;
	static int sum=0;
	static void display()
	{
		if(n<=10)
		{
			sum=sum+n;
			display();
		}
	}
	static void showcount()
	{
		System.out.println(sum);
	}
}
public class Recursionoddnumdemo {
	public static void main(String[]args) {
		Oddnum.shownumbers();
		System.out.println("++++++");
		Oddnum.shownumber();
		System.out.println("++++++");
		Oddnum.shownumber1();
		System.out.println("++++++");
		Oddnum.shownumber2();
		System.out.println("++++++");
		Oddnum.shownumber3();
		System.out.println("++++++");
		Oddnum.display();
		Oddnum.showcount();


	}
}


